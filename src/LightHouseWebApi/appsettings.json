{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Minio": {
    "Endpoint": "localhost:9008",
    "BucketName": "lighthouse-bucket",
    "UserSSL": false
  },
  "VaultSettings": {
    "Address": "http://localhost:8200",
    "Token": "root",
    "MountPoint":"secret"
  },

  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Caching": {
    "UseRedis": true
  },  
  "Redis": {
    "Configuration": "localhost:6379",
    "InstanceName": "LightHouseApp:"
  },
  "ElasticSearchSetttings": {
    "Url": "http://localhost:9200",
    "IndexFormat": "lighthouse-logs-{0:yyyy.MM.dd}",
    "Username": "",
    "Password": ""
  }
}
